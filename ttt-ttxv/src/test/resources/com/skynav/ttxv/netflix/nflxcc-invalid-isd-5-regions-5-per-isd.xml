<?xml version="1.0" encoding="utf-8"?>
<tt xml:lang=""
    xmlns="http://www.w3.org/ns/ttml"
    xmlns:ttp="http://www.w3.org/ns/ttml#parameter"
    xmlns:tts="http://www.w3.org/ns/ttml#styling"
    tts:extent="640px 480px"
    ttp:profile="http://www.netflix.com/ns/ttml/profile/nflx-cc"
    xmlns:ttva="http://skynav.com/ns/ttv/annotations" ttva:warnOn="all" ttva:expectedErrors="1" ttva:expectedWarnings="0" ttva:model="nflxtt">
  <head>
    <layout>
      <region xml:id="r0" tts:extent="100px 10px" tts:origin="20px 20px"/>
      <region xml:id="r1" tts:extent="100px 10px" tts:origin="30px 30px"/>
      <region xml:id="r2" tts:extent="100px 10px" tts:origin="40px 40px"/>
      <region xml:id="r3" tts:extent="100px 10px" tts:origin="50px 50px"/>
      <region xml:id="r4" tts:extent="100px 10px" tts:origin="60px 60px"/>
    </layout>
  </head>
  <body>
    <div>
      <p begin="1s" dur="1s" region="r0">Cue 0</p>
      <p begin="1s" dur="1s" region="r1">Cue 1</p>
      <p begin="1s" dur="1s" region="r2">Cue 2</p>
      <p begin="1s" dur="1s" region="r3">Cue 3</p>
      <p begin="1s" dur="1s" region="r4">Cue 4</p>
    </div>
  </body>
</tt>
